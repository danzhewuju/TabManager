{
  "manifest_version": 3,
  "name": "Tab Manager",
  "version": "1.0.4",
  "description": "快速管理和删除浏览器标签页的工具",
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Tab Manager"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
} 